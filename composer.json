{
    "name": "jobbrander/infoconnect-php-client",
    "type": "library",
    "description": "Infoconnect API Client for PHP projects",
    "keywords": [
        "jobs",
        "api client",
        "infoconnect"
    ],
    "homepage": "https://github.com/JobBrander/infoconnect-php-client",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Karl Hughes",
            "email": "khughes.me@gmail.com",
            "homepage": "https://github.com/karllhughes"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.1",
        "phpunit/php-code-coverage": "~3.0",
        "mockery/mockery": ">=0.9.4",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "JobBrander\\Clients\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobBrander\\Clients\\Test\\": "tests/src/"
        }
    }
}
